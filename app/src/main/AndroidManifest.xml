<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.heshimastores">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/heshima"
        android:label="heshimastores"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Heshima">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".updatessales">
        <intent-filter>
            <action android:name=".updatessales" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter></activity>
        <activity android:name=".updates">
        <intent-filter>
            <action android:name=".updates" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter></activity>
        <activity android:name=".sales">
            <intent-filter>
                <action android:name=".sales" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>




    </application>


</manifest>